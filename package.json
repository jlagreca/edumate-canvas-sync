{
  "name": "edumate-canvas-sync",
  "version": "1.0.0",
  "private": true,
  "description": "node-powered middleware that serves to link Edumate with Canvas.",
  "main": "app.js",
  "scripts": {
    "setup": "echo 'Creating /csv' && mkdir csv",
    "docker-build": "docker build -t edumate-canvas-sync .",
    "docker-run": "docker run -d --restart=on-failure:5 --memory \"300M\" -v ~/.env/edumate-canvas-sync_DEVELOPMENT.env:/src/.env --name edumate-canvas-sync edumate-canvas-sync",
    "test": "tap-prettify tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neurotech/edumate-canvas-sync.git"
  },
  "author": "Tim Douglas",
  "license": "ISC",
  "dependencies": {
    "canvas-api": "1.0.3",
    "chalk": "1.1.1",
    "csv": "0.4.6",
    "dotenv": "1.2.0",
    "hapi": "10.4.1",
    "moment": "2.10.6",
    "nedb": "1.2.1",
    "node-edumate": "4.0.0",
    "node-schedule": "0.2.9"
  },
  "devDependencies": {
    "tap-prettify": "0.0.2",
    "tape": "4.2.1"
  }
}
